{{> header }}

<section>
  <div class="content-wrapper">
    <div class="container-fluid">
      <!-- Breadcrumbs-->
      <ol class="breadcrumb">
        <li class="breadcrumb-item">
          <a href="/">Edit CDE</a>
        </li>
        <li class="breadcrumb-item active">Edit CDE</li>
      </ol>
      <div class="row mx-3">
        <div class="col-12">
                <h1>Edit CDE</h1>
                <form name="registration" id="cde_edit" action="/cde/{{ output.uuid }}/edit" data-toggle="validator" method="POST">
                <input type="hidden" name="user_id" value="{{ output.user_id }}">
                <div class="form-group required">
                    <label class="control-label">First Name</label>
                    <input class="form-control" value="{{ output.first_name }}" name="firstname" id="first_name" type="text" placeholder="First Name" required>
                    <p class="help-block text-danger"></p>
                </div>
                
                <div class="form-group required">
                    <label class="control-label">Last Name</label>
                    <input class="form-control" value="{{ output.last_name }}" name="lastname" id="last_name" type="text" placeholder="Last Name" required>
                    <p class="help-block text-danger"></p>
                </div>
                              
                <div class="form-group required">
                    <label class="control-label">Age</label>
                    <input class="form-control" value="{{ output.age }}" name="age" id="age" type="text" placeholder="Age" required>
                    <p class="help-block text-danger"></p>
                </div>

                <div class="form-group required">
                    <label class="control-label">Employee ID</label>
                    <input class="form-control" value="{{ output.employee_id }}" name="employee_id" id="employee_id" type="text" placeholder="Employee ID" required>
                    <p class="help-block text-danger"></p>
                </div>

                <div class="form-group required">
                    <label class="control-label">Joining Date</label>
                    <input class="form-control" value="{{ output.joining_date }}" name="joining_date" id="joining_date" type="text" placeholder="Joining Date" required>
                    <p class="help-block text-danger"></p>
                </div>
                
                <div class="form-group">
                   <input class="form-control" value="{{ output.password }}" name="pass_hiden" id="pass_hidden" type="hidden" placeholder="Password" required>
                    <p class="help-block text-danger"></p>
                </div>
                
                <div class="form-group required">
                    <label class="control-label">Email</label>
                    <input class="form-control" value="{{ output.email }}" name="email" id="email" type="email" placeholder="Email Address" required>
                    <p class="help-block text-danger"></p>
                </div>

                <div class="form-group">
                    <label>Password</label>
                    <input class="form-control" value="" name="password" id="password" type="password" placeholder="Password">
                    <input class="form-control" value="{{ output.password }}" name="pass_hiden" id="pass_hidden" type="hidden" placeholder="Password" required>
                    <p class="help-block text-danger"></p>
                </div>

                <div class="form-group required">
                    <label class="control-label">Phone Number</label>
                    <input class="form-control" value="{{ output.mobile }}" name="phone" id="phone" type="tel" placeholder="Phone Number" required>
                    <p class="help-block text-danger"></p>
                </div>
                      
                
                <div id="success"></div>
                <div class="form-group">
                    <button type="submit" class="btn btn-primary btn-xl">Submit</button>
                </div>
                </form>
            </div>
            </div>
        </div>
    </div>
</section>

{{> footer }}