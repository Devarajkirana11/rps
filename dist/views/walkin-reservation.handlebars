<main id="page-content">
    <div class="row row-xl page-title">
        <div class="col-md-12">
            <h3 class="text-black fw-semi-thk">Walk In / New Reservation</h3>
        </div>
    </div>

    <div class="page-container">
        <div class="row row-xl">
            <div class="col-md-12 selectWrapper">
                <form id="walkin-reservation-form" class="walkin_reservation" action='/pms/walkin-reservation?hotel_id={{ hotel_id }}' method="POST">
                    <input type="hidden" name="hotel_id" value="{{ hotel_id }}" id="hotel_id">
                    <input type="hidden" name="hotel_country_code" value="{{ hotel_country_code }}" id="hotel_country_code">
                    <input type="hidden" name="user_uuid" value="{{ user_uuid }}" id="user_uuid">
        
                    {{#if errors}}
                        {{#each errors}}
                            <div class="alert alert-danger"> {{ msg }}</div>
                        {{/each}}
                    {{/if}}
                    
                    <div class="row">
                        <div class="col-md-8">
                            <div id="walkin-wizard" class="tab-container tab-colored-arrow-down tab-top tab-danger">
                                <div class="nav-tabs-container">
                                    <ul class="nav nav-tabs nav-justified" role="tablist">
                                        <li role="presentation" class="active">
                                            <a href="#stay-information" aria-controls="stay-information" role="tab" data-toggle="tab"><i class="fa fa-home fa-fw m-xs-r"></i>Stay Information</a>
                                        </li>
                                        <li role="presentation">
                                            <a href="#guest-information" aria-controls="guest-information" role="tab" data-toggle="tab"><i class="fa fa-home fa-fw m-xs-r"></i>Guest Information</a>
                                        </li>
                                    </ul>
                                </div>

                                <div class="tab-content">
                                    <div role="tabpanel" class="tab-pane active" id="stay-information">
                                        <div class="row row-sm">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                <label class="" for="checkin">Check in *</label>
                                                <input type="text" readonly="true" id="check_in" value="{{ inputs.check_in }}" name="check_in" placeholder="" class="form-control">
                                                </div>
                                            </div>

                                            <div class="col-md-6">
                                                <div class="form-group">
                                                <label class="" for="checkout">Check out *</label>
                                                <input type="text" readonly="true" id="check_out" value="{{ inputs.check_out }}" name="check_out" placeholder="" class="form-control">
                                                </div>
                                            </div>
                                        </div>
                                        <!-- End Row -->

                                        <div class="row row-sm">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                <label class="" for="adults">Adults *</label>
                                                <input type="text" id="adults" value="{{ inputs.adults }}" name="adults" placeholder="" class="form-control">
                                                </div>
                                            </div>

                                            <div class="col-md-6">
                                                <div class="form-group">
                                                <label class="" for="child">Child *</label>
                                                <input type="text" id="kids" value="0" name="child" placeholder="" class="form-control">
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row row-sm">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label class="" for="ota_name">No of rooms</label>
                                                    <div class="select-wrapper">
                                                        <select id="noofrooms" name="no_of_rooms" class="form-control">
                                                            <option value="">Select</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <input type="hidden" name="noofrooms_values" value="{{ inputs.no_of_rooms}}" id="noofrooms_values">
                                            <div class="room-details" id="room_details">
                                            {{#each inputs.room_type as |valuer key|}}
                                                <div class="row row-sm room-details-items">
                                                    
                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label class="" for="type">Type</label>
                                                            <div class="select-wrapper">
                                                                <select data-id="{{ key }}" id="room_type" name="room_type[]" class="form-control room-type-item room_types{{ key }}">
                                                                    <option value="">Select</option>
                                                                    {{#each ../rooms as |value key|}}
                                                                    <option value="{{ value._type._type }}" {{#equal valuer value._type._type }} selected="selected" {{/equal}}>{{#getRoomTypeName value._type._type }}{{/getRoomTypeName}}</option>
                                                                    {{/each}}
                                                                </select>
                                                                
                                                            </div>
                                                        </div>
                                                    </div>


                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label class="" for="type">Room Number</label>
                                                            <div class="select-wrapper">
                                                                <select id="room_no{{ key }}" name="room_number[]" class="form-control">
                                                                    <option value="">Select</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                        <input type="hidden" id="room_price{{ key }}" name="room_price[]" class="room_price form-control">
                                                    </div>


                                                </div>
                                            {{/each}}
                                            </div>
                                        {{!-- </div> --}}
                                        <input type="hidden" id="room_number_values" name="room_number_values" value="{{ inputs.room_no }}">

