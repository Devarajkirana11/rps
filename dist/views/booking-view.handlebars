<link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/themes/smoothness/jquery-ui.css">
<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/jquery-ui.min.js"></script>

{{> nav this}}
{{> header}}

<!-- Begin Page Content -->
		<!-- Begin Page Content -->
<main id="page-content">
	<div class="row row-xl page-title">
        <div class="col-md-12">
            <h3 class="text-black fw-semi-thk">Booking Details</h3>
        </div>
    </div>

	<div class="page-container booking-view-container">
    <table cellpadding="0" cellspacing="0" width="100%"  style="border-collapse:collapse;margin:0;padding:0;width:100%;font-size:15px;color:#373737;">
        <tbody>
        <tr>
            <td style="font-family: Roboto-Regular,BlinkMacSystemFont,-apple-system,'Trebuchet MS',Roboto,Helvetica,Arial,sans-serif; border-collapse: collapse;">
                <table cellpadding="0" cellspacing="0" width="600" align="center">
                    <tbody>
            
                    <tr>
                    </tr>

                    <tr>
                        <td>
                            <table cellpadding="0" cellspacing="0" style="width: 100%;">
                                <tbody>
                                <tr>
                                    <td style="text-align: left; font-size: 14px; text-transform: uppercase;"><small style="font-size: 12px;">Booking ID:</small> <br> <strong style="color: #000;">{{ output.booking.referenceId }}</strong></td>
                                    <td style="text-align: right; font-size: 14px; text-transform: uppercase;"><small style="font-size: 12px;">Booking Date:</small> <br> <strong style="color: #000;">{{ output.booking.bookedOn }}</strong></td>
                                </tr>
                                </tbody>
                            </table>
                        </td>
                    </tr>


                    <tr>
                        <td>
                            <table cellpadding="0" cellspacing="0"  style="width: 100%;">
                                <tr>
                                    <td><h4 style="text-transform: uppercase; margin: 0; color: #000; margin-bottom: 10px;">Hotel Information</h4></td>
                                </tr>

                                <tr>
                                    <td>
                                        <table cellpadding="0" cellspacing="0"  style="width: 100%;">
                                            <tr>
                                                <td class="hotel-information">
                                                    <div style="margin-bottom: 5px;">
                                                        <strong style="font-size: 20px; text-transform: uppercase; color: #000;">
                                                            {{ hotel_details._nida_stay_name }}
                                                        </strong>
                                                    </div>
                                                    {{ hotel_details._address_line1 }}<br/>
                                                    {{ hotel_details._address_line2 }}<br/>
                                                </td>
                                                <td class="hotel-information" valign="top" style="text-align: right;">
                                                    <small style="text-transform: uppercase; font-size: 12px;">Phone</small>
                                                    <br>
                                                    <span style="color: #000;">{{ contact_details._FrontDeskContactDetails.[0]._landline_number }}</span>

                                                    <br>
                                                    <br>

                                                    <small style="text-transform: uppercase; font-size: 12px;">Email</small>
                                                    <br>
                                                    <span style="color: #000;">{{ contact_details._FrontDeskContactDetails.[0]._email }}</span>
                                                </td>
                                            </tr>
                                        </table>
                                    </td>
                                </tr>

                                <tr>
                                    <td>
                                        <hr style="border: none; border-top: solid 1px #ddd; margin: 30px 0;">
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>

                    <tr>
                        <td>
                            <table cellpadding="0" cellspacing="0" style="width: 100%;">
                                <tbody>
                                <tr>
                                    <td><h4 style="text-transform: uppercase; margin: 0; color: #000; margin-bottom: 10px;">Booking Details</h4></td>
                                </tr>

                                <tr>
                                    <td>
                                        <table cellpadding="0" cellspacing="0" style="width: 100%">
                                            <tbody>
                                            <tr>
                                                <td style="text-align: left;">
                                                    <p style="margin: 0; text-transform: uppercase;"><small style="font-size: 12px;">Check In</small></p>
                                                    <div style="margin-bottom: 15px; margin-top: 5px;"><h3 style="margin: 0; color: #ec1c24;"><strong>{{ output.roomReservations.[0].checkIn }}</strong></h3></div>
                                                </td>

                                                <td style="text-align: right;">
                                                    <p style="margin: 0; text-transform: uppercase;"><small style="font-size: 12px;">Check Out</small></p>
                                                    <div style="margin-bottom: 15px; margin-top: 5px;"><h3 style="margin: 0; color: #ec1c24;"><strong>{{ output.roomReservations.[0].checkOut }}</strong></h3></div>
                                                </td>
                                            </tr>
                                            </tbody>
                                        </table>
                                    </td>
                                </tr>

                                <tr>
                                    <td>
                                        <table cellpadding="0" cellspacing="0" style="width: 100%;">
                                            <tbody>
                                            <tr>
                                                <td style="width: 33.33%; text-align: left;" valign="top">
                                                    <p style="margin: 0; text-transform: uppercase;"><small style="font-size: 12px;">Guest Name</small></p>
                                                    <h4 style="margin: 0; margin-top: 5px; color: #000;"><strong>{{ output.booking.guest.firstName }} {{ output.booking.guest.lastName }}</strong></h4>
                                                </td>

                                                <td style="width: 33.33%; text-align: center;" valign="top">
                                                    <p style="margin: 0; text-transform: uppercase;"><small style="font-size: 12px;">No of Nights</small></p>
                                                    <h4 style="margin: 0; margin-top: 5px; color: #000;"><strong>{{ output.roomReservations.[0].numberOfNights }} </strong></h4>
                                                </td>

                                                <td style="width: 33.33%; text-align: right;" valign="top">
                                                    <p style="margin: 0; text-transform: uppercase;"><small style="font-size: 12px;">No of Rooms</small></p>
                                                    <h4 style="margin: 0; margin-top: 5px; color: #000;"><strong>{{ no_of_rooms }}</strong></h4>
                                                </td>
                                            </tr>
                                            </tbody>
                                        </table>
                                    </td>
                                </tr>

                                <tr>
                                    <td>
                                        <hr style="border: none; border-top: solid 1px #ddd; margin: 30px 0;">
                                    </td>
                                </tr>
                                </tbody>
                            </table>
                        </td>
                    </tr>

                    <tr>
                        <td>
                            <table cellpadding="0" cellspacing="0" style="width: 100%;">
                                <tbody>
                                <tr>
                                    <td><h4 style="text-transform: uppercase; margin: 0; color: #000; margin-bottom: 10px;">Tax Invoice</h4></td>
                                </tr>
                                <tr>
                                    <td style="padding: 5px 0; border-bottom: solid 1px #dd; text-align: left;"><small style="font-size: 12px; text-transform: uppercase;">Room Price</small></td>
                                    <td style="padding: 5px 0; border-bottom: solid 1px #dd; text-align: right;"><strong style="color: #000;">{{ paymentData.totalRoomPrice }}</strong></td>
                                </tr>
                                <tr>
                                    <td style="padding: 5px 0; border-bottom: solid 1px #dd; text-align: left;"><small style="font-size: 12px; text-transform: uppercase;">Discount</small></td>
                                    <td style="padding: 5px 0; border-bottom: solid 1px #dd; text-align: right;"><strong style="color: #000;">{{ paymentData.discountAmount }}</strong></td>
                                </tr>
                                <tr>
                                    <td style="padding: 5px 0; border-bottom: solid 1px #dd; text-align: left;"><small style="font-size: 12px; text-transform: uppercase;">Service Fee</small></td>
                                    <td style="padding: 5px 0; border-bottom: solid 1px #dd; text-align: right;"><strong style="color: #000;">{{ paymentData.serviceFee }}</strong></td>
                                </tr>
                                <tr>
                                    <td style="padding: 5px 0; border-bottom: solid 1px #dd; text-align: left;"><small style="font-size: 12px; text-transform: uppercase;">GST / VAT</small></td>
                                    <td style="padding: 5px 0; border-bottom: solid 1px #dd; text-align: right;"><strong style="color: #000;">{{ paymentData.tax }}</strong></td>
                                </tr>
                                <tr>
                                    <td style="padding: 5px 0; border-bottom: solid 1px #dd; text-align: left;"><small style="font-size: 12px; text-transform: uppercase;">Tourism Tax</small></td>
                                    <td style="padding: 5px 0; border-bottom: solid 1px #dd; text-align: right;"><strong style="color: #000;">{{ paymentData.tourismTax }}</strong></td>
                                </tr>
                                <tr>
                                    <td style="padding: 10px 0 0 0; border-bottom: solid 1px #dd; text-align: left;"><small style="font-size: 12px; text-transform: uppercase;">Total Price</small></td>
                                    <td style="padding: 10px 0 0 0; border-bottom: solid 1px #dd; text-align: right;"><strong style="color: #ec1c24; font-size: 20px;">{{ paymentData.paymentCurrency }} {{ paymentData.totalAmount }}</strong></td>
                                </tr>
                                <tr>
                                    <td>
                                        <hr style="border: none; border-top: solid 1px #ddd; margin: 30px 0;">
                                    </td>
                                    <td>
                                        <hr style="border: none; border-top: solid 1px #ddd; margin: 30px 0;">
                                    </td>
                                </tr>
                                </tbody>
                            </table>
                        </td>
                    </tr>

                    {{#if audit_details }}
                    <tr>
                        <td><h4 style="text-transform: uppercase; margin: 0; color: #000; margin-bottom: 10px;">Payment Summary</h4></td>
                    </tr>
                    <tr>
                        <td>
                        <table cellpadding="0" cellspacing="0" style="width: 100%;">
                            <thead>
                                <tr>
                                    <td><b>Date</b></td>
                                    <td><b>Description</b></td>
                                    <td><b>Amount</b></td>
                                </tr>
                                <tr><br/></tr>
                            </thead>
                            <tbody>    
                                {{#each audit_details }}
                                    <tr>
                                        <td> {{ this.transactionDate }} </td>
                                        <td> {{ this.transactedItems }}</td>
                                        <td> {{ this.amount }}</td>
                                    </tr>
                                    <tr><br/></tr>
                                {{/each}}
                            </tbody>
                        </table>
                        </td>
                    </tr>
                    {{/if}}
                    </tbody>
                </table>
            </td>
        </tr>
        </tbody>
    </table>

	</div>
</main>

<div style="float:left;width:100%;position:relative;">
	{{> footer}}
</div>